<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="form" action="/compose" method="post">
  <label>Title:</label>
  <input class="form-control" type="text" name="blogTitle" autofocus autocomplete="off">
  <br>
  <textarea class="form-control" name="blogBody" rows="8" cols="80"></textarea>
  <br>
  <button class="btn btn-sm btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>
